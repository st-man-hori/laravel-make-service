{
    "name": "st-man-hori/laravel-make-service",
    "description": "create service class and service interface for laravel",
    "type": "library",
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "StManHori\\LaravelMakeService\\LaravelMakeServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "StManHori\\LaravelMakeService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
